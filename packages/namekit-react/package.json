{
  "name": "@namehash/namekit-react",
  "description": "Namehash Namekit React Components.",
  "version": "0.3.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/namehash/namekit.git",
    "directory": "packages/namekit-react"
  },
  "keywords": [
    "ENS",
    "NameKit",
    "NameHash"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch"
  },
  "sideEffects": false,
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "styles": "./dist/index.css",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "default": "./dist/index.mjs",
      "styles": "./dist/index.css"
    },
    "./styles.css": {
      "import": "./dist/index.css",
      "default": "./dist/index.css"
    }
  },
  "dependencies": {
    "@namehash/ens-webfont": "workspace:*",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "autoprefixer": "10.4.19",
    "classcat": "5.0.4",
    "postcss": "8.4.38",
    "tailwindcss": "3.4.4",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ]
}
